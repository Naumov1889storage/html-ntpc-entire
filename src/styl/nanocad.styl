      display: flex
      justify-content: space-around
      flex-wrap: wrap
      li {
        margin: 15px 10px 15px 10px
        a {
          position: relative
          display: block
          padding-top: 60px
          font-family: Calibri, Segoe, "Segoe UI", Optima, Arial, sans-serif
          font-size: 16px
          font-weight: 400
          line-height: 20px
          color: #4984ad
          text-align: center
          &:before {
            content: ''
            position: absolute
            top: 0
            left: 50%
            transform: translateX(-50%)
            width: 50px
            height: 50px
            background: url(/img/icon-nanocad1.png) no-repeat center
            background-size: contain
          }
        }
        &:nth-child(1) {

        }
        &:nth-child(2) {
          a {
            &:before {
              background-image: url(/img/icon-nanocad2.png)
            }
          }
        }
        &:nth-child(3) {
          a {
            &:before {
              background-image: url(/img/icon-nanocad3.png)
            }
          }
        }
        &:nth-child(4) {
          a {
            &:before {
              background-image: url(/img/icon-nanocad2.png)
            }
          }
        }
        &:nth-child(5) {
          a {
            &:before {
              background-image: url(/img/icon-nanocad1.png)
            }
          }
        }

      }
    }
  }

  &__call {
    margin-top: 38px
    h3 {
      margin-bottom: 30px
    }
  }
}

@media (max-width: 1040px) {
  .nanocad2 {

    .tabs1 {
      .tabs__title {
        li {
          a {
            font-size: 20px
          }
        }
      }
    }
  }
}
  
@media (max-width: 800px) {
    .nanocad2 {
      .nanocad2__product {
        flex-direction: column
        .left {
          margin: 0
          width: 100%
          height: auto
          .img {
            width: 100%
            height: auto
          }
        }
      }
    }
}

@media (max-width: 600px) {
  .nanocad2 {
    .tabs1 {
      .tabs__content {
        #tabs-1 {
          img, p {
            padding-left: 15px
            padding-right: 15px
          }
        }
      }
    }
  }
}
/*NANOCAD2 END*/


/*ENGSOFT BEGIN*/
.engsoft {
  margin-bottom: 65px
  &__types {
    .container {
      display: flex
      flex-wrap: wrap
    }
    a {
      position: relative
      margin: 5px 15px 5px 0
      padding: 0 13px 4px 13px
      height: 37px
      font-family: Calibri, Segoe, "Segoe UI", Optima, Arial, sans-serif
      font-size: 16px
      font-weight: 400
      line-height: 37px
      text-decoration: none
      border: 1px solid #4984ad
      background-color: #ffffff
      &:hover {
        //color: darken(#0f73b9, 50%)
        border-color: #dd7f68//darken(#4984ad, 50%)
      }
      &:first-child {
        &:before {
          z-index: 1
          background: #ffffff
        }
      }
      &:last-child {
        &:after {
          z-index: 1
          background: #ffffff
        }
      }
      &:before, &:after {
        content: ''
        position: absolute
        z-index: -1
        top: 50%
        left: -701px
        width: 700px
        height: 1px
        background: #4984ad
      }
      &:after {
        left: auto
        right: -701px
      }
    }
  }

  &__content {
    margin-bottom: 30px
    .container {
      display: flex
      flex-wrap: wrap
    }
    .item {
      display: flex
      flex-direction: column
      align-items center
      margin: 35px 0 35px 0
      min-width: 230px
      width: 25%
      & > a {
        min-height: 240px
        text-align: center

        font-family: latobold
        font-size: 20px
        color: #417aa3
        img {
          display: block
          margin: 16px auto 16px auto
        }
      }
      .accordion {
        max-width: 230px
        width: 100%
        &.active {
          .content {
            padding: 33px 0 15px 0
            max-height: 620px
            border-bottom: 1px solid #9e9e9e
            border-top: 1px solid #9e9e9e
          }
          .toggler {
            span:nth-child(1) {
              display: none
            }
            span:nth-child(2) {
              display: block
            }
          }
          &--additinal {
            .content {
              border-bottom: 1px solid #9e9e9e
              border-top: 1px solid #9e9e9e
            }
          }
        }
        .toggler {
          position: relative
          margin: 0 auto
          max-width: 122px
          span:nth-child(2) {
            display: none
          }
        }
        .content {
          max-height: 0
          margin-top: -13.5px
          padding: 0
          border: 1px solid #9e9e9e
          border-radius: 4px
          border-top: none
          border-bottom: none
          overflow: hidden
          transition: .4s ease-out
          li {
            a {
              display: block
              padding: 0 20px 0 20px
              font-family: Calibri, Segoe, "Segoe UI", Optima, Arial, sans-serif;
              font-size: 16px;
              font-weight: 400;
              line-height: 20px;
            }
          }
        }
      }
    }
  }

  &__content2 {
    margin-top: 48px
    h3 {
      margin-bottom: 13px
      font-family: latoblack
      font-size: 20px
      line-height: 32px
      color: #4984ad
    }
    p,
    p > a {
      font-family: Calibri
      font-size: 16px
      font-weight: 400
      line-height: 20px
      color: #545454
    }
    p > a {
      color: #417aa3
    }
  }

}

@media (max-width: 970px) {
  .engsoft {
    &__content {
      .item {
        width: 33%
      }
    }
  }
}

@media (max-width: 780px) {
  .engsoft {
    &__content {
      .item {
        width: 50%
      }
    }
  }
}

@media (max-width: 530px) {
  .engsoft {
    &__content {
      .item {
        width: 100%
      }
    }
  }
}
/*ENGSOFT END*/
