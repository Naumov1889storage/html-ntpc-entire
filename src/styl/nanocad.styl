.nanocad {
  margin-bottom: 65px

  .tabs1 {
    .tabs__content {
      margin-top: -10px
      & > div > h2 {
        margin-bottom: 30px
      }
    }
  }

  .ecp__tel {
    height: 47px
    input {
      border: 1px solid #4984ad
      &::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
        color: #4984ad
        opacity: 1; /* Firefox */
      }
      &:-ms-input-placeholder { /* Internet Explorer 10-11 */
        color: #4984ad;
      }
      &::-ms-input-placeholder { /* Microsoft Edge */
        color: #4984ad;
      }
    }
    button {
      background-color: #4984ad
    }
  }

  .btn3 {
    padding-bottom: 2px
    font-family: Lato
    font-size: 16px
    font-weight: 400
    color: #000000
    text-decoration: none
  }

  p, p > a, p > b {
    font-family: Calibri, Segoe, "Segoe UI", Optima, Arial, sans-serif !important
    font-size: 16px !important
    font-weight: 400
    line-height: 20px !important
    color: #545454 !important
  }
  p > a {
    color: #417aa3 !important
  }
  p > b {
    font-size: 15px !important
  }

  h1 {
    margin-top: 35px
    font-family: latoblack
    font-size: 26px
    line-height: 32px
    color: #34546c
  }
  h2 {
    padding: 0 15px 0 15px
    color: #4984ad
    font-family: latoblack
    font-size: 20px
    line-height: 32px
    text-align: center
  }
  .container > h3 {
    font-family: latoblack
    font-size: 20px
    line-height: 32px
    color: #4984ad
    text-align: center
  }
  .tabs1 .tabs__content h3, h3 > a {
    font-family: latoblack
    font-size: 20px
    font-weight: 400
    line-height: 32px
    color: #4984ad
  }

  .downloadImg,
  .downloadImg span {
    position: relative
    display: block
    margin-bottom: 30px
    padding: 4px 0 4px 50px//4px 20px 4px 90px
    font-family: Calibri
    font-size: 16px
    font-weight: 400
    line-height: 20px
    color: #545454
    text-decoration: none
    .size {
      margin: 0
      padding: 0
      color: #989898
      &:before {
        display: none
      }
    }
    &:before {
      content: ''
      position: absolute
      top: 0
      left: 0//41px
      width: 36px
      height: 48px
      background: url(/img/icon-document.png) no-repeat center
    }
  }

  .form__number {
    font-family: latoblack
    font-size: 80px
    color: #545454
    text-align: center
  }
  h3.form__title {
    margin: 10px 15px 27px 15px
    width: auto
    font-family: latoblack !important
    font-size: 27px !important
    color: #545454 !important
    text-align: center
    span {
      display: block
      font-family: latoregular
      font-size: 16px
      font-weight: 400
    }
  }
  &__form.form__submit {
    margin: 0 auto
    padding-top: 30px
    padding-bottom: 22px
    max-width: 300px
    min-width: auto
    width: 100%
    border: 1px solid #c7c7c7
    background: #f9f9f9
    input {
      margin-bottom: 11px
      background-color: #ffffff !important
      &.email,
      &.organization {
        background: #ffffff url(/img/iconset-nanocad2.png) no-repeat 12px 10px
      }
      &.organization {
        background-position-y: -41px
      }
    }
    button {
      margin: 12px 0 11px 0
    }
    .checkbox.checkbox--circle {
      width: 100%
      label {
        color: #666666
        line-height: 22px
      }
    }
  }

  &__modal.modal {
    max-width: 340px
    &:before {
      display: none
    }
    .modal__close {
      top: 3px
      right: 1px
      font-family: latoblack
      font-size: 14px
      color: #666666
      border: none
    }

    h3.form__title {
      margin: 35px auto 0 auto
      font-size: 22px !important
    }
    
    .nanocad__form.form__submit {
      margin: 0 auto
      padding-top: 20px
      background: #ffffff
      border: none
    }
    .nanocad__form2 {
      margin: 0 auto 34px auto
      max-width: 254px
    }

  }

  &__form2 {

    position: relative

    .blocker {
      position: absolute
      z-index: 1
      width: 100%
      height: 100%
      background: #ffffff
      opacity: .4
    }
    
    table, th, td {
      border: 1px solid #cecece;
      box-sizing: border-box
    }
    table {
      margin: 0 auto
      max-width: 430px
      width: 100%
      border-collapse: collapse;
      tr{
        td {
          padding: 6px
          font-family: Calibri, Segoe, "Segoe UI", Optima, Arial, sans-serif
          font-size: 16px
          font-weight: 400
          color: #666666
          &:first-child {
            img {
              display: block
              margin: auto
              max-width: 32px
              max-height: 32px
            }
          }
          &:nth-child(2) {
            padding: 6px 11px 6px 11px
          }
          button.btn3 {
            width: 100%
          }
        }
        &:first-child {
          td:first-child {
            display: none
          }
        }
      }

    }
    .checkbox.checkbox--circle {
      display: inline-block
      label {
        padding-left: 20px !important
      }
      input:not(checked) + label:before,
      input:not(checked) + label:after,
      input:checked + label:after {
        left: 2px
      }
    }
  }

  .feedback {
    .divider {
      margin-left: -100px !important
      width: 200%
    }
    .item {
      display: flex
      padding: 22px 0 22px 0//22px 70px 22px 40px
      .left {
        margin-right: 40px
        min-width: 125px
      }
    }
  }

  .gallery {
    margin: 25px 0 0 0//25px 40px 0 40px
    text-align: center
    .current {
      line-height: 0
      font-size: 0
    }
    .set {
      img {
        margin: 2px 1px 2px 1px
        width: 118px
        height: 62px
        cursor: pointer
        &.active {
          border: 2px solid #00a2d6
        }
      }
    }
  }

}

/*NANOCAD1 BEGIN*/
.nanocad1.nanocad {

  .nanocad1__img {
    margin-top: 50px
    margin-bottom: 110px
    text-align: center
  }

  .tabs1 {
    .divider {
      margin: 0
    }
  }

  #tabs-1 {
    .item {
      display: flex
      padding: 23px 0 23px 0//23px 30px 23px 32px
      .price {
        font-family: latoregular
        font-size: 18px
        font-weight: 400
        line-height: 20px
        color: #545454
        b {
          font-family: latoblack
          color: #545454
        }
      }
      .btn3 {
        margin: 10px 17px 0 0
      }
      .left {
        margin-right: 27px
        min-width: 100px
        text-align: center
      }
    }
  }

  #tabs-2 {
    .content {
      //padding: 0 35px 0 35px
      h3 {
        margin-top: 18px
      }
      p {
        margin: 10px 0 17px
      }
    }
  }

  #tabs-3 {
    ul {
      margin: 22px 0 22px 0
      li {
        position: relative
        padding: 8px 0 8px 40px//8px 35px 8px 61px
        font-family: latoregular
        font-size: 16px
        font-weight: 400
        line-height: 20px
        color: #545454
        b {
          font-size: 16px
          font-family: latoblack
        }
        &:before {
          content: ''
          position: absolute
          top: 7px
          left: 0//22px
          width: 30px
          height: 30px
          background: url(/img/iconset-nanocad.png) no-repeat center 3px
        }
        &:nth-child(2) {
          &:before {
            background-position-y: -36px
          }
        }
        &:nth-child(3) {
          &:before {
            background-position-y: -77px
          }
        }
        &:nth-child(4) {
          &:before {
            background-position-y: -116px
          }
        }
        &:nth-child(5) {
          &:before {
            background-position-y: -158px
          }
        }
        &:nth-child(6) {
          &:before {
            background-position-y: -197px
          }
        }
        &:nth-child(7) {
          &:before {
            background-position-y: -236px
          }
        }
        &:nth-child(8) {
          &:before {
            background-position-y: -278px
          }
        }
        &:nth-child(9) {
          &:before {
            background-position-y: -317px
          }
        }
        &:nth-child(10) {
          &:before {
            background-position-y: -356px
          }
        }

      }
    }

    .link {
      padding: 8px 0 8px 0//8px 35px 8px 24px
      font-family: latoregular
      font-size: 16px
      line-height: 20px
      color: #545454
      a {
        color: #4984ad
      }
      &.first {
        padding-top: 28px
      }
    }
  }


  &__img {
    text-align: center
  }

  #tabs-5 {
    p {
      margin: 12px 0 12px 0//12px 42px 12px 42px
      letter-spacing: -0.13px
      & > b {
        letter-spacing: -0.13px
      }
    }
    .right {
      position: relative
      .blocker {
        position: absolute
        z-index: 1
        width: 100%
        height: 100%
        background: #ffffff
        opacity: .4
      }
    }
  }

}

@media (max-width: 970px) {
  .nanocad {
    .feedback .right {
      margin-top: 0
    }
  }
}

@media (max-width: 850px) {
    .nanocad {
      .nanocad__getprod {
        flex-direction: column
        .left {
          margin-bottom: 30px
        }
      }
    }
}
  
@media (max-width: 600px) {
    .nanocad1.nanocad {
      #tabs-1 {
        .item {
          display: block
          //padding-left: 15px
          //padding-right: 15px
          .left {
            float: left
            margin-bottom: 10px
          }
          .right {
            h3 {
              width: 100%
            }
          }
        }
      }
      #tabs-2 {
        .content {
          //padding-left: 15px
          //padding-right: 15px
        }
      }
    }
    .nanocad {
      .feedback {
        .item {
          display: block
          //padding-left: 15px
          //padding-right: 15px
          .left {
            float: left
            margin-right: 15px
            margin-bottom: 10px
          }
        }
      }

      .gallery {
        //margin-left: 15px
        //margin-right: 15px
      }
    }
}

  
@media (max-width: 400px) {
  .nanocad1.nanocad {
    #tabs-1 {
      .item {
        display: flex
        flex-direction: column
        .left {
          float: none
          margin-right: 0
        }
      }
    }
  }
  .nanocad {
    .feedback {
      .item {
        display: flex
        flex-direction: column
        .left {
          float: none
        }
      }
    }

    .nanocad__getprod {
      .nanocad__form2 {
        table {
          tr {
            td {
              .checkbox.checkbox--circle {
                display: block
              }
              .btn3 {
                min-width: 80px
              }
              &:first-child {
                padding: 0
                max-width: 0
                max-height: 0
                font-size: 0
                line-height: 0
                border: none
                opacity: 0
                overflow: hidden
              }
            }
          }
        }
      }
    }
  }
}
/*NANOCAD1 END*/

/*NANOCAD2 BEGIN*/
.nanocad2 {

  .divider {
