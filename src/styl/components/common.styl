* {
  font-family: latoregular
}

html,
body {
  height: 100%;
}

body {
  display: flex;
  flex-direction: column;
  height: 100%;
}
.toPushFooter {
  flex: 1 0 auto;
}
.footer {
  flex: 0 0 auto;
}

.container {
  margin: 0 auto
  padding: 0 15px 0 15px
  max-width: 1030px
  width: 100%
}

a {
  color: #0f73b9
  text-decoration: underline
  &:hover {
    text-decoration: none
  }
}

h2.innerPage__title {
  margin-top: 40px
  font-family: latoblack
  font-size: 26px
  line-height: 32px
  color: #34546c
}

.crumbs {
  margin-bottom: 40px//25px
  a {
    font-family: latoregular
    font-size: 16px
    line-height: 32px
    color: #000000;
    &.current {
      pointer-events: none
      cursor: default
      text-decoration: none
    }
  }
  .slash {
    margin: 0 4px 0 4px
    font-family: TimesNewRomanPSMT
    &:before {
      display: none
    }
  }
}

.slash {
  &:before {
    content: '/'
    margin: 0 5px 0 5px
  }
}



form.form__submit {
  display: flex
  flex-direction: column
  align-items: center
  position: relative
  padding: 10px 30px 0 30px
  min-width: 290px
  width: 300px
  background: #ffffff
  h3 {
    margin: 13px 0 20px 0
    font-family: latoblack
    font-size: 23px
    line-height: 25px
    color: #5a7eba
  }
  input,
  textarea {
    margin-bottom: 17px
    padding: 0 5px 0 40px
    max-width: 227px
    width: 100%
    height: 40px
    border: 1px solid #ababab
    border-radius: 4px
    font-family: latoregular
    font-size: 16px
    line-height: 25px
    box-sizing border-box
    &::placeholder {
      color: #666666;
      opacity: 1;
    }

    &:-ms-input-placeholder {
      color: #666666;
    }

    &::-ms-input-placeholder {
      color: #666666;
    }
  }
  .input {
    position: relative
    &:before {
      content: ''
      position: absolute
      top: 0
      left: 15px
      width: 17px
      height: 17px
      background: black
    }
  }
  input {
    &.name,
    &.tel {
      background: url(/img/iconset-feedback1.jpg) no-repeat 12px 10px
    }
    &.tel {
      background-position: 12px -46px
    }
  }
  textarea {
    padding: 5px 12px 5px 12px
    height: 105px
    resize: none
  }

  .agreement {
    color: #848586
    font-family: latoregular
    font-size: 12px
    line-height: 25px
    a {
      color: #848586
    }
    label {
      padding-left: 22px
      &:before {
        top: -1px
      }
    }
    input:not(checked) + label:before {
      top: 4px
      width: 15px
      height: 15px
      background: url(/img/icon-checkbox.png) no-repeat
      background-size: cover
      border-radius: 0
    }
    input:not(checked) + label:after {
      top: 50%;
      margin-top: -6px;
      left: 5px;
      width: 3px;
      height: 6px;
      background: transparent
      border-right: 1px solid #ffffff
      border-bottom: 1px solid #ffffff
      border-radius: 0
      transform: rotate(45deg)
    }
    input[type='checkbox'] {
      height: 0
    }
  }
}


.pagination {
  display: flex
  justify-content: center
  flex-wrap: wrap
  margin: 0 10px 76px 10px
  .pag {
    display: flex
    justify-content: center
    align-items: center
    margin: 5px
    min-width: 40px
    height: 40px
    line-height: 40px
    font-size: 20px
    color: #000000
    text-decoration: none
    border: 1px solid rgba(216, 216, 216, 0.8)
    &.arrow {
      padding-bottom: 3px
      font-family: TimesNewRomanPSMT
    }
    &:hover {
      border: 1px solid rgba(67, 115, 170, 0.8)
    }
    &.active {
      border: 1px solid rgba(67, 115, 170, 0.8)
    }
  }
}

#topmenu__burger {
  display: none
  position: relative
  width: 100%
  height: 55px
  background: rgba(67,115,170,0.4)
  .burger {
    top: 50%
    left: 0
    &.active {
      i {
        background: transparent
      }
    }
    i {
      background: white
      &:before,
      &:after {
        background: white
      }
    }
  }
}

@media (max-width: 430px) {
  #topmenu__burger {
    position: static !important
    display: block
    .burger {
      //position: static
    }
  }
}


.btn2 {
  position: relative
  width: 103px
  height: 35px
  font-size: 16px
  line-height: 35px
  color: #ffffff
  border-radius: 4px
  background-color: #6dadda
  cursor: pointer
  &:before {
    content: ''
    position: absolute
    z-index: -1
    left: 0
    top: 0
    bottom: -5px
    width: 100%
    height: 35px
    background-color: #4984ad;
    border-radius: 4px
  }
  &:after {
    content: ''
    position: absolute
    z-index: -2
    left: -4px
    right: -4px
    top: -4px
    bottom: -4px
    background-color: #b3cada
    border-radius: 4px
  }
  &--red {
    z-index: 0
    padding: 0 14px 0 14px
    width: auto
    background: rgba(221, 127, 104, 0.4)
    &:before {
      background-color: #dd7f68
    }
    &:after {
      z-index: -1
      background: rgba(221, 127, 104, 0.4)
    }
  }
}
.btn3 {
  display: flex
  align-items: center
  justify-content: center
  min-width: 113px
  height: 27px

  font-family: Calibri, Segoe, "Segoe UI", Optima, Arial, sans-serif;
  font-size: 16px;
  font-weight: 400;
  line-height: 20px;
  color: #545454;

  border: 1px solid #9e9e9e
  border-radius: 4px
  background-color: #fcf4c0
  cursor: pointer
  &--white {
    background: #ffffff
  }
}


