.checkbox {
  display: flex
  height: auto
  &--picture {
    label {
      display: flex
      flex-direction: column
      align-items: center
      justify-content: center
      padding: 0 !important
      text-align: center
      &:hover {
        span.img__wrap {
          &:before {
            opacity: 1
          }
        }
      }
      img {
        position: relative
        z-index: 1
      }
      span.img__wrap {
        position: relative
        margin: auto 0 0 0
        &:before {
          content: ''
          position: absolute
          z-index: 0
          top: 50%
          left: 50%
          //margin-left: -50%
          //transform: translateY(-50%)
          margin: -41.5px 0 0 -41.5px
          width: 83px
          height: 83px
          background-color: #f7e0da
          border-radius: 100%
          opacity: 0
          //transition: .1s
        }
      }
      span.text {
        min-height: 55px
        margin-top: auto
        font-family: Calibri, Segoe, "Segoe UI", Optima, Arial, sans-serif
        font-size: 18px
        font-weight: 400
        color: #545454
      }
    }

    input:not(checked) + label:before,
    input:not(checked) + label:after {
      display: none
    }
    input:checked + label .img__wrap {
      &:before {
        background: #d76546
        opacity: 1
      }
    }
  }
  &--cirle {
    label {
      padding-left: 31px !important
    }
    input:not(checked) + label:before {
      left: 8px
      top: 3px
      width: 15px
      height: 15px
      background: url(/img/iconset-checkbox.png) no-repeat center -20px
      transition: none !important
    }
    input:not(checked) + label:after,
    input:checked + label:after {
      left: 8px
      top: 3px
      margin: 0
      width: 15px
      height: 15px
      background: url(/img/iconset-checkbox.png) no-repeat center 0
      transition: none !important
    }
    input:not(checked) + label:after {
      opacity: 0
    }
    input:checked + label:before {
      opacity: 0
    }

  }
}


